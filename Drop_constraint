DECLARE
    v_constraint_name VARCHAR2(30);
BEGIN
    -- Find the check constraint for the specific column
    SELECT constraint_name INTO v_constraint_name
    FROM user_constraints
    WHERE table_name = 'YOUR_TABLE_NAME'
        AND constraint_type = 'C'
        AND search_condition LIKE '%YOUR_COLUMN_NAME%'
        AND search_condition LIKE '%IN%'  -- Look for ENUM-like constraints
        AND ROWNUM = 1;
    
    -- Drop the constraint
    EXECUTE IMMEDIATE 'ALTER TABLE YOUR_TABLE_NAME DROP CONSTRAINT ' || v_constraint_name;
    
    DBMS_OUTPUT.PUT_LINE('Dropped constraint: ' || v_constraint_name);
EXCEPTION
    WHEN NO_DATA_FOUND THEN
        DBMS_OUTPUT.PUT_LINE('No ENUM-like constraint found for the specified column.');
    WHEN OTHERS THEN
        DBMS_OUTPUT.PUT_LINE('Error: ' || SQLERRM);
END;
/
